-->

<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-1500</x_min>
		<x_max>1500</x_max>
		<y_min>-1500</y_min>
		<y_max>1500</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>

	<overall>
		<max_time units="min">4320</max_time>
		<time_units>min</time_units>
		<space_units>micron</space_units>

		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>
	</overall>

	<parallel>
		<omp_num_threads>8</omp_num_threads>
	</parallel>

	<save>
		<folder>output</folder> <!-- use . for root -->

		<full_data>
			<interval units="min">30</interval>
			<enable>true</enable>
		</full_data>

		<SVG>
			<interval units="min">30</interval>
			<enable>true</enable>
		</SVG>

		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>

	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
		<!--<virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>-->
	</options>

	<microenvironment_setup>
		<variable name="virus" units="virions/micron" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">1.4766</diffusion_coefficient> 	<!-- D_virus  -->
				<decay_rate units="1/min">0.0029</decay_rate>  															<!-- lambda_virus -->
			</physical_parameter_set>
			<initial_condition units="none">0</initial_condition>
			<Dirichlet_boundary_condition units="none" enabled="true">0</Dirichlet_boundary_condition>
		</variable>

		<variable name="wall" units="mmHg" ID="1">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">0</diffusion_coefficient>
				<decay_rate units="1/min">0</decay_rate>
			</physical_parameter_set>
			<initial_condition units="mmHg">0</initial_condition>
			<Dirichlet_boundary_condition units="mmHg" enabled="true">0</Dirichlet_boundary_condition>
		</variable>

		<variable name="chemokine" units="pg/ml" ID="2">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">555.56</diffusion_coefficient> 	<!--  D_chemokine  -->
				<decay_rate units="1/min">8.06e-6</decay_rate> 															<!--  lambda_chemokine  -->
			</physical_parameter_set>
			<initial_condition units="none">0</initial_condition>
			<Dirichlet_boundary_condition units="none" enabled="true">0</Dirichlet_boundary_condition>
		</variable>

		<options>
			<calculate_gradients>true</calculate_gradients>
			<track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
			<!-- not yet supported -->
			<initial_condition type="matlab" enabled="false">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			<!-- not yet supported -->
			<dirichlet_nodes type="matlab" enabled="false">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>

	<cell_definitions>

		<cell_definition name="TH cell" ID="1">
			<phenotype>
				<cycle code="1" name="Basic Ki67 cycle model">
					<phase_transition_rates units="1/min">
						<rate start_index="3" end_index="2" fixed_duration="false">0.000079026</rate> 	<!--r_01_CD4 : TH_prolif_rate-->
						<rate start_index="2" end_index="3" fixed_duration="false">0.00143</rate> 			<!--r_10:Division rate-->
					</phase_transition_rates>
				</cycle>
				<death>
					<model code="100" name="apoptosis">
						<death_rate units="1/min">0</death_rate>
					</model>

				</death>
				<volume>
				</volume>
				<mechanics>
					<cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength> 	<!-- c_j^{ccr} -->
				</mechanics>

				<motility>
					<speed units="micron/min">4</speed> 																									<!--nu-->
					<persistence_time units="min">0</persistence_time> 																		<!--from a distribution-->
					<migration_bias units="dimensionless">0</migration_bias> 															<!--from a distribution-->
				</motility>

				<secretion>
					<substrate name="chemokine">
						<secretion_rate units="1/min">0.0417</secretion_rate><!--S_chemokine_CD4:chemokine_secretion_rate-->
					</substrate>
				</secretion>

				<molecular />
			</phenotype>
			<custom_data>
			</custom_data>
		</cell_definition>

		<cell_definition name="cancer cell" ID="2">
			<phenotype>
				<cycle code="5" name="live">
					<phase_transition_rates units="1/min">
						<rate start_index="0" end_index="0" fixed_duration="false">0.000735</rate> <!--beta : GBM_cell_proliferation_rate -->
					</phase_transition_rates>
				</cycle>
				<death>
					<model code="100" name="apoptosis">
						<death_rate units="1/min">0.00001</death_rate>
					</model>
				</death>
				<volume>
				</volume>
				<mechanics>
					<cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">1.25</relative_maximum_adhesion_distance>
					<options>
						<set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
					</options>
				</mechanics>

				<motility>
					<speed units="micron/min">0.25</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">.5</migration_bias>

					<options>
						<enabled>false</enabled>
						<chemotaxis>
							<enabled>true</enabled>
							<substrate>signal C</substrate>
							<direction>1</direction>
						</chemotaxis>
					</options>
				</motility>

				<secretion>
					<substrate name="signal B">
						<secretion_rate units="1/min">1</secretion_rate>
					</substrate>
				</secretion>

				<molecular />
			</phenotype>
			<custom_data>
			</custom_data>
		</cell_definition>

		<cell_definition name="C" ID="3" parent_type="default">
			<phenotype>
				<cycle code="5" name="live">
					<phase_transition_rates units="1/min">
						<rate start_index="0" end_index="0" fixed_duration="false">0.00072</rate>
					</phase_transition_rates>
				</cycle>
				<death>
					<model code="100" name="apoptosis">
						<death_rate units="1/min">0.00001</death_rate>
					</model>

					<model code="101" name="necrosis">
						<death_rate units="1/min">0.01</death_rate>
					</model>
				</death>
				<volume>
				</volume>
				<mechanics>
					<cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">1.25</relative_maximum_adhesion_distance>
					<options>
						<set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
					</options>
				</mechanics>

				<motility>
					<speed units="micron/min">0.25</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">.5</migration_bias>

					<options>
						<enabled>false</enabled>
						<chemotaxis>
							<enabled>true</enabled>
							<substrate>signal A</substrate>
							<direction>1</direction>
						</chemotaxis>
					</options>
				</motility>

				<secretion>
					<substrate name="signal C">
						<secretion_rate units="1/min">1</secretion_rate>
					</substrate>
				</secretion>

				<molecular />
			</phenotype>
			<custom_data>
			</custom_data>
		</cell_definition>

	</cell_definitions>



	<initial_conditions>
		<cell_positions type="csv" enabled="true">
			<folder>./config</folder>
			<filename>cells.csv</filename>
		</cell_positions>
	</initial_conditions>

	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed>

		<!-- for virus -->
		<rhostar_virus type="double" units="virus/micron">268.51</rhostar_virus>			<!-- virus_saturation_density -->
		<V0 type="double" units="dimensionless">3.0248</V0>														<!-- initial_virus_density -->
		<u_g type="double" units="1/min">0.002</u_g>																	<!-- GBM_virus_uptake_rate -->
		<m_half type="double" units="1/min">10</m_half>																<!-- infection_threshold -->
		<gamma type="double" units="1/min">0.0081</gamma>															<!-- virus_replication_rate   0.4886 -->
		<alpha type="double" units="virus">6600</alpha>																<!-- virus_burst_number -->
		<rho_max type="double" units="virus">0.0125</rho_max>													<!-- maximum density of virus that a cell can uptake -->
		<delta_V type="double" units="virus">0.1466</delta_V> 												<!-- viral_decay_from_burst_cell -->
		<!--for gbm cells-->
		<u_g type="double" units="1/min">0.02</u_g> 													<!-- GBM_virus_uptake_rate -->
		<N_GBM_sparse type="int" units="cells">5580</N_GBM_sparse> 						<!--  initial_GBM_cells_sparse  -->
		<N_GBM_dense type="int" units="cells">12556</N_GBM_dense> 						<!--  initial_GBM_cells_dense -->
		<!--for stroma cells-->
		<u_s type="double" units="1/min">1</u_s> 															<!-- stroma_virus_uptake_rate -->
		<N_stroma_sparse type="int" units="cells">14306</N_stroma_sparse> 		<!--  initial_stroma_cells_spars  -->
		<N_stroma_dense type="int" units="cells">2861</N_stroma_dense> 				<!--  initial_stroma_cells_dense  -->
		<!--for immune cells-->
		<nu_max type="double" units="micron/min">24.6</nu_max>										<!-- CTL_max_speed -->
		<nu_star type="double" units="dimensionless">0.005</nu_star>							<!-- Chemokine_motility_EC50 -->
		<b_CD8 type="double" units="dimensionless">1</b_CD8>											<!-- CTL_chemokine_migration_bias -->
		<S_chemokine_CD4 type="double" units="1/min">0.0417</S_chemokine_CD4>			<!-- chemokine_secretion_rate -->
		<TH_prolif_increase_due_to_stimulus type="double" units="dimensionless">100</TH_prolif_increase_due_to_stimulus>
		<CTL_prolif_increase_due_to_stimulus type="double" units="dimensionless">100</CTL_prolif_increase_due_to_stimulus>
		<d_attach type="double" units="1/min">50</d_attach> 											<!--  CD8 to infected cell attachment distance  -->
		<rhostar_chemokine type="double" units="ng/micron">5</rhostar_chemokine> 	<!--  chemokine_saturation_density  -->
		<tau type="double" units="min">50</tau> 																	<!--  time taken to kill cell  -->
		<!-- domain -->
		<R type="double" units="micron">1270</R>																<!-- tumour radius -->
		<kappa type="double" units="dimensionless">1.208</kappa>								<!-- factor by which the radius of patches increases -->
		<proportion type="double" units="dimensionless">0.7</proportion>				<!-- dense to sparse ratio -->
		<xi type="double" units="cell/micron">0.0039</xi>												<!-- maximum_cell_density -->
		<!-- other -->
		<f_F type="double" units="dimensionless">0.75</f_F>
		<elastic_coefficient type="double" units="1/min">0.01</elastic_coefficient>
		<!-- for tmz -->
		<k_a type="double" units="1/min">0.096666666667</k_a>									<!-- constant absorption rate -->
		<k_23 type="double" units="1/min">0.000012</k_23>											<!-- transfer rate from plasma to CSF -->
		<k_32 type="double" units="1/min">0.012666666667</k_32>								<!-- transfer rate form CSF to plasma -->
		<V_p type="double" units="ml">140</V_p>																<!-- volume of distribution in the CSF (fixed) -->
		<V_D type="double" units="ml">30300</V_D>															<!-- volume of the distribution of the central compartement -->
		<CL type="double" units="ml/min">166.66666667</CL>										<!-- oral clearance -->
		<!-- should be implemented in matlab instead -->
		<c_1 type="double" units="mg">332.5</c_1>								<!-- constant of integration 1 -->
		<IC50 type="double" units="mg/ml">0.0194</IC50>

	</user_parameters>

</PhysiCell_settings>
